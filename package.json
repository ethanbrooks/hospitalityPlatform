		"build:tizenPackage": "cd platforms/browser/ && /Users/ethanbrooks/tizen-studio/tools/ide/bin/tizen.sh build-web && /Users/ethanbrooks/tizen-studio/tools/ide/bin/tizen.sh package -t wgt -- platform-tizen/",
