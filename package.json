{
 "name": "hotel_herrera",
 "version": "0.0.0",
 "main": "src/main",
 "scripts": {
  "start": "ng serve",
  "build": "ng build ng build --prod --aot --build-optimizer --delete-output-path --output-path=dist --subresourceIntegrity=true",
  "lint": "ng lint hotelHerrera",
  "test": "ng test",
  "e2e": "ng e2e",
  "build:development:ios": "ionic cordova build ios",
  "build:development:android": "ionic cordova build android",
  "build:production:ios": "ionic cordova build ios --prod --release",
  "build:production:android": "ionic cordova build android --prod --release",
  "build:themes": "devextreme build",
  "build:postinstall": "npm run build:themes",
  "deploy": "yarn run build:prerender && dev-bin/deploy.sh hotelherrera.com pp",
  "build:client-and-server-bundles": "ng build --prod --aot --build-optimizer --delete-output-path --subresourceIntegrity=true && ng run hotelHerrera:server:production",
  "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
  "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
  "compile:server": "tsc -p server.tsconfig.json",
  "generate:prerender": "cd dist && node prerender",
  "serve:prerender": "cd dist/browser && http-server",
  "serve:ssr": "node dist/server"
 },
 "private": true,
 "pre-commit": [],
