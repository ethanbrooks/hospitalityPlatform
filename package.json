		"build:prerender": "node platforms/ssr/prerender",
		"serve:prerender": "cd platforms/browser && http-server",
		"build:ssr": "npm run build:client-and-server-bundles && npm run build:server",
		"build:server": "tsc -p server.tsconfig.json",
		"serve:ssr": "node platforms/ssr/server",
