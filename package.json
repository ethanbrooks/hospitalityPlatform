{
	"name": "hotel_herrera",
	"version": "0.0.0",
	"main": "src/main",
	"scripts": {
		"start": "ng serve",
		"build": "ng build --prod --aot --delete-output-path --output-path=dist --subresourceIntegrity=true",
		"lint": "ng lint hotelHerrera",
		"test": "ng test",
		"e2e": "ng e2e",
		"build:themes": "devextreme build",
		"build:postinstall": "npm run build:themes",
		"build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
		"build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
		"build:client-and-server-bundles": "ng build --prod --build-optimizer --prod --delete-output-path --output-path=dist --subresourceIntegrity=true && ng run hotelHerrera:server:production",
		"deploy": "yarn run build && dev-bin/deploy.sh hotelherrera.com pp",
		"compile:server": "tsc -p server.tsconfig.json",
		"generate:prerender": "cd dist && node prerender",
		"serve:prerender": "cd dist/browser && http-server",
		"serve:ssr": "node dist/server"
	},
	"private": true,
	"pre-commit": [],
	"dependencies": {
		"@angular-devkit/core": "8.2.0",
		"@angular/animations": "~8.2.0",
		"@angular/cdk": "^8.1.2",
		"@angular/common": "~8.2.0",
		"@angular/compiler": "~8.2.0",
		"@angular/core": "~8.2.0",
		"@angular/elements": "^8.2.0",
		"@angular/forms": "~8.2.0",
		"@angular/platform-browser": "~8.2.0",
		"@angular/platform-browser-dynamic": "~8.2.0",
		"@angular/pwa": "^0.802.0",
		"@angular/router": "~8.2.0",
		"@angular/service-worker": "^8.2.0",
		"@nguniversal/common": "^8.1.1",
		"@nguniversal/module-map-ngfactory-loader": "^8.1.1",
		"@ngx-translate/core": "^11.0.1",
		"@ngx-translate/http-loader": "^4.0.0",
		"core-js": "^3.1.4",
		"devextreme": "latest",
		"devextreme-angular": "latest",
		"devextreme-schematics": "latest",
		"msgpack5": "4.2.1",
		"ramda": "^0.26.1",
		"redux": "^4.0.4",
		"reflect-metadata": "^0.1.13",
		"rxjs": "~6.5.2",
		"rxjs-compat": "^6.5.2",
		"tslib": "^1.10.0",
		"uppy": "^1.3.0",
		"video.js": "^7.6.0",
		"videojs-overlay": "^2.1.4",
		"videojs-playlist": "^4.3.1",
		"zone.js": "~0.10.1"
	},
	"devDependencies": {
		"@angular/platform-server": "~8.2.0",
		"@nguniversal/express-engine": "^8.1.1",
		"@angular-devkit/build-angular": "^0.802.0",
		"@angular/cli": "~8.2.0",
		"@angular/compiler-cli": "~8.2.0",
		"@angular/language-service": "~8.2.0",
		"@types/jasmine": "~3.3.16",
		"@types/jasminewd2": "~2.0.6",
		"@types/node": "~12.6.9",
		"codelyzer": "^5.1.0",
		"css-loader": "^3.1.0",
		"devextreme-cli": "latest",
		"http-server": "^0.11.1",
		"jasmine-core": "~3.4.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~4.2.0",
		"karma-chrome-launcher": "~3.0.0",
		"karma-cli": "^2.0.0",
		"karma-coverage-istanbul-reporter": "~2.1.0",
		"karma-jasmine": "~2.0.1",
		"karma-jasmine-html-reporter": "^1.4.2",
		"pre-commit": "^1.2.2",
