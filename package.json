		"lint": "ng lint app",
		"test": "ng test",
		"e2e": "ng e2e",
		"deploy": "yarn run build:prerender && dev-bin/deploy.sh hotelherrera.com pp",
		"build:development:ios": "ionic cordova build ios",
		"build:development:android": "ionic cordova build android",
		"build:production:ios": "ionic cordova build ios --prod --release",
		"build:production:android": "ionic cordova build android --prod --release",
		"serve:production:electron": "ionic cordova emulate electron --prod --release",
		"serve:production:ios": "ionic cordova emulate ios --prod",
		"serve:production:android": "ionic cordova emulate android --prod",
		"serve:production:electron": "ionic cordova emulate electron --prod",
		"serve:ionic": "ionic serve -l",
		"build:tizenPackage": "cd www/ && /Users/ethanbrooks/tizen-studio/tools/ide/bin/tizen.sh build-web && /Users/ethanbrooks/tizen-studio/tools/ide/bin/tizen.sh package -t wgt -- platform-tizen/",
		"serve:tizenRun": "cd www/ && sdb install platform-tizen/angularTizenSample.wgt && sdb shell  \"wrt-launcher '-s Bsl3EwnDc9.angularTizenSample'\"",
		"build:client-and-server-bundles": "ng build --prod --delete-output-path --subresourceIntegrity=true && ng run app:server:production",
		"build:prerender": "cd dist && node prerender",
		"build:ssr": "npm run build:client-and-server-bundles && npm run build:server",
		"build:server": "tsc -p server.tsconfig.json",
		"serve:prerender": "cd dist/browser && http-server",
		"serve:ssr": "node dist/server",
		"build:themes": "devextreme build",
		"build:postinstall": "npm run build:themes"
	},
